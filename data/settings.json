{
  "osticketBaseUrl": "https://tickets.welkeptbooks.com",
  "taskUrlPattern": "/scp/tasks.php?id={taskId}",
  "calendar": {
    "colors": {
      "openTaskDue": "#0d6efd",
      "closedTaskDue": "#6c757d",
      "futureCreation": "#20c997",
      "futureDue": "#ffc107"
    },
    "defaults": {
      "showOpenTaskDue": true,
      "showFutureCreation": true,
      "showFutureDue": true
